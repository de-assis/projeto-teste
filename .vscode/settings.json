{
    "workbench.startupEditor": "newUntitledFile",
    "editor.fontSize": 14,
    "editor.lineHeight": 1.8,
    "editor.fontFamily": "JetBrains Mono",
    "editor.fontLigatures": true,
    "editor.tabSize": 2,
    "editor.rulers": [
        80,
        120
    ],
    "editor.stickyScroll.enabled": false,
    "editor.parameterHints.enabled": false,
    "editor.renderLineHighlight": "gutter",
    "editor.semanticHighlighting.enabled": false,
    "editor.minimap.enabled": false,
    "editor.scrollbar.vertical": "auto",
    "editor.hideCursorInOverviewRuler": true,
    "editor.accessibilitySupport": "off",
    "editor.suggestSelection": "first",
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": "explicit"
    },
    "[json]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "editor.acceptSuggestionOnCommitCharacter": false,
    "typescript.updateImportsOnFileMove.enabled": "always",
    "typescript.suggest.autoImports": true,
    "typescript.preferences.preferTypeOnlyAutoImports": true,
    "javascript.suggest.autoImports": true,
    "explorer.compactFolders": false,
    "explorer.confirmDelete": false,
    "explorer.confirmDragAndDrop": false,
    "explorer.sortOrder": "foldersNestsFiles",
    "explorer.fileNesting.enabled": true,
    "explorer.fileNesting.patterns": {
        "package.json": ".eslint*, prettier*, tsconfig*, vite*, pnpm-*, bun.lockb, nest*, package-lock*",
        "tailwind.config.*": "tailwind.config*, postcss.config*",
        ".env.local": ".env*",
        ".env": ".env*"
    },
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/.DS_Store": true,
        "**/CVS": true
    },
    "files.associations": {
        ".env.*": "dotenv",
        ".prettierrc": "json",
        "*.css": "css",
        ".dev.vars": "dotenv"
    },
    "symbols.files.associations": {
        "*.module.ts": "nest",
        "*.guard.ts": "typescript",
        "*.spec.ts": "ts-test",
        "*.e2e-spec.ts": "ts-test",
        "*.mock.ts": "ts-test",
        "vitest.config.e2e.ts": "vite",
        ".env.development.local": "gear",
        ".env.test.local": "gear",
        ".env.local": "gear",
        ".env.example": "gear"
    },
    "workbench.iconTheme": "material-icon-theme",
    "workbench.statusBar.visible": true,
    "workbench.activityBar.location": "hidden",
    "workbench.editor.labelFormat": "short",
    "workbench.layoutControl.enabled": true,
    "workbench.editor.empty.hint": "text",
    "workbench.preferredDarkColorTheme": "Min Dark",
    "workbench.preferredLightColorTheme": "Min Light",
    "window.autoDetectColorScheme": true,
    "window.commandCenter": true,
    "window.titleBarStyle": "native",
    "terminal.integrated.fontSize": 14,
    "terminal.integrated.fontFamily": "JetBrainsMono Nerd Font",
    "terminal.integrated.showExitAlert": false,
    "terminal.integrated.gpuAcceleration": "on",
    "git.enableSmartCommit": true,
    "git.openRepositoryInParentFolders": "always",
    "gitlens.codeLens.recentChange.enabled": false,
    "gitlens.codeLens.authors.enabled": false,
    "extensions.ignoreRecommendations": true,
    "eslint.validate": [
        "javascript",
        "javascriptreact",
        "graphql"
    ],
    "cSpell.language": "en,pt",
    "cSpell.userWords": [
        "automations",
        "chakra",
        "cloudflare",
        "dayjs",
        "dotenv",
        "Elysia",
        "esbuild",
        "fastify",
        "feedbackwidget",
        "gamificada",
        "hono",
        "jamjuree",
        "ksuid",
        "liveblocks",
        "Marguerita",
        "middlewares",
        "mixpanel",
        "nestjs",
        "nivo",
        "omni",
        "postgres",
        "reactflow",
        "rsxp",
        "skylab",
        "sqlite",
        "supergraph",
        "sympla",
        "tailwindcss",
        "tiptap",
        "trpc",
        "tsup",
        "unform",
        "usuario",
        "webm",
        "WEBPUSH"
    ],
    "cSpell.enableFiletypes": [
        "!cpp",
        "!python",
        "!java",
        "!php"
    ],
    "emmet.includeLanguages": {
        "javascript": "javascriptreact",
        "mdx": "javascriptreact"
    },
    "emmet.syntaxProfiles": {
        "javascript": {}
    },
    "files.autoSave": "afterDelay",
    "files.autoSaveDelay": 1000
}